{
	"name": "vue-blox",
	"version": "0.2.1",
	"author": "Adam Eisfeld",
	"license": "Apache-2.0",
	"description": "Render custom Vue components from regular JS objects at runtime. Bind reactive properties from your components to variables.",
	"keywords": [
		"vue",
		"json",
		"block",
		"blockkit",
		"dynamic"
	],
	"homepage": "https://github.com/AdamEisfeld/vue-blox#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AdamEisfeld/vue-blox.git"
	},
	"bugs": {
		"url": "https://github.com/AdamEisfeld/vue-blox/issues"
	},
	"files": [
		"dist"
	],
	"main": "dist/vue-blox.umd.js",
	"module": "dist/vue-blox.es.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"dev": "vite --config ./demo/vite.demo.config.ts",
		"build": "vite build && npm run build:types",
		"build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist ",
		"typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"test-ci": "vitest run --coverage.enabled --coverage.reporter='text-summary'"
	},
	"peerDependencies": {
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitest/coverage-c8": "^0.25.8",
		"@vue/test-utils": "^2.2.6",
		"@vue/tsconfig": "^0.1.3",
		"jsdom": "^20.0.3",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-dts": "^1.7.1",
		"vitest": "^0.25.8",
		"vue": "^3.2.45",
		"vue-tsc": "^1.0.11",
		"mustache": "^4.2.0"
	},
	"devDependencies": {
		"@babel/types": "^7.20.5",
		"@types/node": "^18.11.13",
		"@types/mustache": "^4.2.2"
	}
}
